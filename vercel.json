{
  "buildCommand": "npm run build",
  "outputDirectory": "dist/public",
  "redirects": [
    { "source": "/cigna-cigna-coverage", "destination": "/psychiatrist-orlando-accepts-cigna", "statusCode": 308 },
    { "source": "/aetna-aetna-coverage", "destination": "/psychiatrist-orlando-accepts-aetna", "statusCode": 308 },
    { "source": "/medicare-medicare-coverage", "destination": "/medicare-psychiatrist-orlando", "statusCode": 308 },
    { "source": "/blue-cross-blue-shield-blue-cross-blue-shield-coverage", "destination": "/blue-cross-blue-shield-therapy-orlando", "statusCode": 308 }
  ],
  "rewrites": [
    { "source": "/robots.txt", "destination": "/api/index" },
    { "source": "/sitemap.xml", "destination": "/api/index" },
    { "source": "/sitemap_index.xml", "destination": "/api/index" },
    { "source": "/image-sitemap.xml", "destination": "/api/index" },
    { "source": "/api/(.*)", "destination": "/api/index" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
